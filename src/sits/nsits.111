CRCL2:	MOV #1,(A)+	;SET IN THE REFENCE COUNT
	CLR (A)+	;SAY THERE IS NO CONSUMER
	CLR (A)+	;CLEAR THE COUNT OF VALID DATA
	MOV E,(A)+	;SET IN THE NUMBER OF ENTRIES
