	REST (A)	;PUT IN CAPABILITY OF MS CAPABILITY TO FAULTING SPHERE
	MOV ITEM0,A	;COPY THE ITEM NO. OF THE FAULT HANDLER
	MOV ITEM2,B	;THE SUPERIOR SPHERE
	JSR PC,PUTPRS	;PUT THE PROCESS INTO THE SPHERE
